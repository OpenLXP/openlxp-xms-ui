    "browserslist": "^4.16.5",
    "immer": "^9.0.6",
    "ionicons": "^5.5.2",
    "moment": "^2.29.2",
    "next": "12.1.0",
    "node-forge": "^1.3.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.2.0",
    "tailwind": "^4.0.0",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start"
  },
  "eslintConfig": {
